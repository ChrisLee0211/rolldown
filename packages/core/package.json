{
  "name": "@rolldown/core",
  "version": "1.0.0",
  "description": "rolldown core binding",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:types && yarn build:lib",
    "build:lib": "cargo run --example build_rolldown_core",
    "build:types": "yarn run -T tsc --emitDeclarationOnly",
    "type-check": "yarn run -T tsc --noEmit"
  },
  "dependencies": {
    "@rolldown/node-binding": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "rollup": "^3.17.2",
    "type-fest": "^3.6.0"
  }
}
